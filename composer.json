{
    "name": "kiwilan/php-rss",
    "description": "PHP package to generate RSS feeds with presets.",
    "version": "0.3.02",
    "keywords": [
        "php",
        "rss",
        "feed",
        "podcast",
        "atom",
        "xml"
    ],
    "homepage": "https://github.com/kiwilan/php-rss",
    "license": "MIT",
    "authors": [
        {
            "name": "Ewilan Rivi√®re",
            "email": "ewilan.riviere@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/array-to-xml": "^3.1"
    },
    "require-dev": {
        "kiwilan/php-xml-reader": "^0.2.31",
        "laravel/pint": "^1.2",
        "pestphp/pest": "^1.20",
        "phpstan/phpstan": "^1.10",
        "spatie/invade": "^2.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Kiwilan\\Rss\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kiwilan\\Rss\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-filter": "vendor/bin/pest --filter",
        "test-parallel": "vendor/bin/pest --parallel",
        "test-coverage": "vendor/bin/pest --coverage --min=90",
        "test-coverage-parallel": "vendor/bin/pest --parallel --coverage",
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
